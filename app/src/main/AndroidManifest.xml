<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.normsusbr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_norms"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_norms_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NormsUSBR">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SectionNormActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/section_activity_title">
        </activity>

        <activity
            android:name=".DetailsActivity"
            android:label="@string/detail_activity_title">
        </activity>

        <activity
            android:name=".DeveloperActivity"
            android:parentActivityName=".SectionNormActivity"
            android:label="@string/developer">
        </activity>

        <activity
            android:name=".DiamondSawActivity"
            android:parentActivityName=".SectionNormActivity"
            android:label="@string/diamond_saw_title"/>

    </application>

</manifest>